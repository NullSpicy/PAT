#include <stdio.h>
#include <bits/stdc++.h>
#include <iostream>
using namespace std;

int main()
{
    int a,b;
    cin>>a>>b;
    int sum=a+b;
    int p;
    if(sum<0)
    {
        cout<<'-';
        sum=sum*(-1);
    }
    p=sum;
    int i=0;
    while(p!=0)
    {
        i++;
        p=p/10;
    }
    int x[i];
    p=sum;

    int j=0;
    while(p!=0)
    {
        x[j]=(p%10);
        p=p/10;
        j++;
    }

    for(int n=i-1;n>=0;n--)
    {
        cout<<x[n];
        if(n%3==0&&n!=0)
            cout<<',';
    }
}
